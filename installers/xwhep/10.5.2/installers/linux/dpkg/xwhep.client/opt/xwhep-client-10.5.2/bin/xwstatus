PRG=$0

############################################################
# Let first reach "real" file through symbolic links, if any
############################################################

while [ -h "$PRG" ]; do
    ls=`ls -ld "$PRG"`
    link=`expr "$ls" : '^.*-> \(.*\)$' 2>/dev/null`
    if expr "$link" : '^/' 2> /dev/null >/dev/null; then
        PRG="$link"
    else
        PRG="`dirname $PRG`/$link"
    fi
done

progdir=`dirname $PRG`
progname=`basename $0`


######################################################
# Main
######################################################

PROG="xtremweb.client"
#ROOTDIR="$progdir/.."

currentDir=`pwd`
cd $progdir
ROOTDIR=`pwd`/..
cd $currentDir


$ROOTDIR/bin/xwworks $*
