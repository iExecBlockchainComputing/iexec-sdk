    "npm": ">=8"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand --detectOpenHandles --forceExit",
    "build": "npm run generate-json-es-modules && npm run generate-dist && npm run generate-packages && npm run generate-doc",
    "lint": "eslint src",
    "generate-json-es-modules": "rimraf src/common/generated && node generateEsModulesFromJson.cjs",
    "generate-dist": "rimraf dist && mkdir dist && mkdir dist/esm && cp -r src/. dist/esm/",
    "generate-packages": "node generatePackages.cjs",
    "generate-doc": "npm run generate-lib-doc && npm run generate-cli-doc",
    "generate-lib-doc": "./node_modules/typedoc/bin/typedoc --tsconfig tsconfig.doc.json --disableSources",
    "generate-cli-doc": "node generateCliDoc.cjs"
  },
